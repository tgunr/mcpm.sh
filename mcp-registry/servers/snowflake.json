  },
  "license": "NOT GIVEN",
  "categories": [
    "database"
  ],
  "tags": [
    "snowflake",
    "sql",
    "database"
  ],
  "installations": {
    "uvx": {
      "type": "uvx",
      "command": "uvx",
      "args": [
        "mcp_snowflake_server",
        "--account",
        "the_account",
        "--warehouse",
        "the_warehouse",
        "--user",
        "the_user",
        "--password",
        "their_password",
        "--role",
        "the_role",
        "--database",
        "the_database",
        "--schema",
        "the_schema"
      ]
    }
  }
}